package homework03;

import java.util.GregorianCalendar;

/**
 * A few simple tests to evaluate the functionality of the Inventory, DatedItem, and Item classes
 * 
 * @author	thehoule, ccortello
 * @date	1/21/14
 *
 */

public class InventoryTester {

	public static void main(String[] args) {
		// initialize an inventory
		Inventory MilkInventory = new Inventory();
		Item Milk = (new Item("milk"));
		
		MilkInventory.addItem(Milk, new GregorianCalendar(2,2,2), 2);
		
		
		
		System.out.println("Done!");
	}
}
